{
  "name": "react-mobx-starter",
  "description": "A starter boilerplate for an universal web application with the best development experience and best practices.",
  "main": "index.js",
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0"
  },
  "keywords": [
    "starter",
    "boilerpate",
    "universal",
    "react",
    "mobx",
    "react router v4",
    "express",
    "webpack v3",
    "es6",
    "hot reloading",
    "babel",
    "postcss",
    "jest",
    "unit testing",
    "performance optimizing"
  ],
  "author": "swufewyd <yadong.wu@socialcredits.cn>",
  "license": "MIT",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run clean:build && better-npm-run build",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:js": "better-npm-run lint:js",
    "lint:style": "better-npm-run lint:style"
  },
  "betterScripts": {
    "start": {
      "command": "nodemon ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "backendApi": "http://192.168.31.116:6060"
      }
    },
    "build": {
      "command": "webpack --progress --hide-modules --config ./tools/webpack/config.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "lint:js": {
      "command": "eslint ./src ./tools ./index.js"
    },
    "lint:style": {
      "command": "stylelint \"./src/**/*.scss\""
    },
    "clean:build": {
      "command": "rimraf ./public/assets"
    }
  },
